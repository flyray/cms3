<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.qllt.po.RecruitRoom" table="recruitroom">
  <id name="recruitRoomID"><generator class="identity"/> </id>
  <property generated="never" lazy="false" name="recruitRoomName"/>
  <property generated="never" lazy="false" name="recruitRoomBelong"/>
  <property generated="never" lazy="false" name="recruitRoomState"/>
 </class>
</hibernate-mapping>
