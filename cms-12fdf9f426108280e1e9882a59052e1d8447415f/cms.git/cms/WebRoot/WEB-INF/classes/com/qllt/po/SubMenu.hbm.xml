<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.qllt.po.SubMenu" table="submenu">
        <id name="subMenuID">
 			<generator class="identity"></generator>
 		</id>       
 		<property name="subMenuName"></property>
       	<property name="subMenuOrder"></property>
       	<property name="createTime"></property>
       	<property name="subMenuLink"></property>
       	<property name="menuID"></property>
    </class>
</hibernate-mapping>
